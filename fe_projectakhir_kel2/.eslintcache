mail}</span>\n                            <input disabled={disableAll?true:false} type=\"text\" className=\"form-control my-2\" onChange={(e)=>changeOtp(e)} placeholder=\"Masukkan OTP\" style={{transition:'500ms'}} />\n                            {isOtpDone&&!isVerified?\n                                <span style={{\n                                    cursor:isOtpSent?\"not-allowed\":\"pointer\",\n                                    color:\"blue\"}} \n                                    className='my-2' onClick={onsendclick}>\n                                        Confirm OTP\n                                </span>\n                                :\n                                null\n                            }\n                        </>\n                        :\n                        null\n                    }\n                    {isVerified?\n                    <>\n                        {renderInputPassword()}\n                    </>\n                    :\n                    null\n                    }\n                </div>\n                <div style={{padding:5}}>\n                    {disableAll?\n                    <Link to=\"/login\">\n                        <Button color=\"primary\"> Login </Button>\n                    </Link>\n                        :\n                    <Button disabled={isPasswordPass?false:true} color=\"primary\" onClick={()=>resetPassword()}> Save </Button>\n                    }\n                </div>\n            </div>\n        </div>\n\n    )\n}\n\n\n","/Users/BD/Documents/GitHub/FE_ProjectAkhir_Kel2/fe_projectakhir_kel2/src/helpers/index.jsx",[],"/Users/BD/Documents/GitHub/FE_ProjectAkhir_Kel2/fe_projectakhir_kel2/src/pages/admin/parcels/manageparcel.jsx",[],"/Users/BD/Documents/GitHub/FE_ProjectAkhir_Kel2/fe_projectakhir_kel2/src/App.js",["192","193","194"],"import './App.css';\nimport React,{useEffect,useState} from 'react';\nimport Login from './pages/login/login'\nimport Home from './pages/home