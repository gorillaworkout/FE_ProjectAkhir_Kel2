.log(res)\n        setPaymentInWaiting(res.data)\n    }).catch((err)=>{\n      console.log(err)\n    })\n  },[pages])\n\n  const onConfirmClick=(payment_id,transaksi_id)=>{\n    Axios.post(`${API_URL_SQL}/payment/confirmpayment`,{payment_id:payment_id,transaksi_id:transaksi_id})\n    .then((res)=>{\n        console.log(res)\n        setmaxpages(res.data.length)\n        Axios.get(`${API_URL_SQL}/payment/getpaymentwaiting?page=${pages}`)\n        .then((res)=>{\n            console.log(res)\n            setPaymentInWaiting(res.data)\n        }).catch((err)=>{\n          console.log(err)\n        })\n        setPaymentInWaiting(res.data)\n    })\n  }\n\n  const renderTable=()=>{\n    return paymentInWaiting.map((val,index)=>{\n      return(\n        <TableRow key={val.payment_id} className={classes.subcontainer}>\n            <TableCell style={{width:130}} align=\"center\">{val.payment_id}</TableCell>\n            <TableCell style={{width:130}} align=\"center\">{val.transaksi_id}</TableCell>\n            <TableCell style={{width:200}}>\n              <div style={{maxWidth:'200px'}}>\n                <ReactImageMagnify {...{\n                        smallImage: {\n                            alt: 'Payment',\n                            isFluidWidth: true,\n                            src: API_URL_SQL+val.image\n                        },\n                        largeImage: {\n                            src: API_URL_SQL+val.image,\n                            width: 600,\n                            height: 600\n                        },\n                        enlargedImageContainerDimensions:{\n                            width:\"300%\",\n                            height:\"130%\"\n                        }\n                    }} />\n                {/* <img width='100%' height='100%' src={val.image} alt={val.namatrip}/> */}\n              </div>\n            </TableCell>\n            <TableCell style={{width:170}} align=\"center\">{moment(val.tanggaltransaksi).format('Do MMMM YYYY')}</TableCell>\n            <TableCell style={{width:170}} align=\"center\">{moment(val.tglexp).format('Do MMMM YYYY')}</TableCell>\n            <TableCell style={{width:160}} align=\"center\">Rp {numeral(val.totalpayment).format('0,0.0')}</TableCell>\n            <TableCell style={{width:110}} align=\"center\">\n              <button onClick={()=>onConfirmClick(val.payment_id,val.transaksi_id)}>Confirm</button>\n            </TableCell>\n        </TableRow>\n      )\n    })\n  }\n  const pindahpage=(a)=>{\n    console.log(a)\n    setpages(a)\n  }\n  const renderpaging=()=>{\n    console.log(\"jalan\")\n    console.log(maxpages)\n    var jumlahpage=Math.ceil(maxpages/3)\n    var arr=new Array(jumlahpage)\n    console.log(jumlahpage)\n    console.log(arr)\n    for(let i=0;i<arr.length;i++){\n      if((i+1)===pages){\n        arr[i]=(<PaginationItem key={i} disabled>\n                  <PaginationLink>\n                    {i+1}\n                  </PaginationLink>\n                </PaginationItem>)\n      }else{\n        console.log(i)\n        arr[i]=(\n          <PaginationItem key={i} onClick={()=>pindahpage(i+1)}>\n                <PaginationLink>\n                  {i+1}\n                </PaginationLink>\n              </PaginationItem>\n        )\n      }\n    }\n    return arr\n  }\n\n  return (\n    <>\n      <div className=\"user-container\">\n      <HeaderAdmin/>\n        <div className=\"user-right\">\n            <div className=\"header-user\">\n                <div className=\"icon-group\">\n                    <FaMoneyCheckAlt className=\"icon-user\" color=\"black\"/>\n                    <p style={{fontWeight:'600'}}>Payment</p>\n                </div>\n            </div>\n            <div className=\"container-data\">\n              <TableContainer>\n                <Table>\n                  <TableHead>\n                    <TableRow>\n                        <TableCell align=\"center\">Payment Id</TableCell>\n                        <TableCell align=\"center\">Transaksi Id</TableCell>\n                        <TableCell align=\"center\">Bukti Transfer</TableCell>\n                        <TableCell align=\"center\">Tgl Transaksi</TableCell>\n                        <TableCell align=\"center\">Tgl Expired</TableCell>\n                        <TableCell align=\"center\">Total Harus Dibayar</TableCell>\n                        <TableCell align=\"center\"></TableCell>\n                    </TableRow>\n                  </TableHead>\n                  <TableBody>\n                      {renderTable()}\n                  </TableBody>\n                </Table>\n                <Pagination style={{display:\"flex\", justifyContent:\"center\",width:\"100%\"}}>\n                  {renderpaging()}\n                </Pagination>\n              </TableContainer>\n            </div>\n        </div>\n      </div>\n      </>\n    \n\n  );\n}\n \nexport default AdminPayment;",["130","131"],"/Users/BD/Documents/GitHub/FE_ProjectAkhir_Kel2/fe_projectakhir_kel2/src/pages/admin/categoryParcel.jsx",["132","133"],"import React, { Component ,createRef} from 'react';\nimport HeaderAdmin from './../../components/header/headerAdmin'\nimport {MdAddShoppingCart} from 'react-icons/md'\nimport {GiMilkCarton} from 'react-icons/gi'\nimport Axios from 'axios'\nimport { API_URL_SQL } from '../../helpers/apiUrl';\nimport Table from '@material-ui/core/Table';\nimport TableBody from '@material-ui/core/TableBody';\nimport TableCell from '@material-ui/core/TableCell';\nimport TableContainer from '@material-ui/core/TableContainer';\nimport TableHead from '@material-ui/core/TableHead';\nimport TableRow from '@material-ui/core/TableRow';\nimport { Button, Modal, ModalHeader, ModalBody, ModalFooter } from 'reactstrap';\nimport ReactPaginate from 'react-paginate';\n\nclass CategoryParcel extends Component {\n    state = {  \n        categoryParcel:[],\n        setModalParcel:false,\n        nama:createRef(),\n        offset:0,\n        data:[],\n        perPage:7,\n        currentPage:0,\n        slice:[]\n    }\n    componentDidMount(){\n        Axios.get(`${API_URL_SQL}/product/getallcatparcel`)\n        .then((res)=>{\n            console.log(res.data)\n            this.setState({categoryParcel:res.data})\n            this.pagination()\n        }).catch((err)=>{\n            console.log(err)\n        })\n    }\n  \n    onAddDataParcel=()=>{\n        console.log('button jalan')\n        this.setState({setModalParcel:true})\n    }\n\n    onDelete=(id)=>{\n        let id2= id\n        console.log('delete jalan')\n        Axios.post(`${API_URL_SQL}/product/deletecatparcel`,{\n            id:id2\n        }).then((res)=>{\n            console.log('berhasil delete')\n            console.log(res.data)\n            this.setState({categoryParcel:res.data})\n            this.pagination()\n        }).catch((err)=>{\n            console.log(err)\n        })\n    }\n\n    onSave=()=>{\n        var {nama}=this.state\n        var nama = nama.current.value\n        \n        Axios.post(`${API_URL_SQL}/product/addCatParcel`,{\n            nama\n        }).then((res)=>{\n            console.log(res.data)\n            this.setState({categoryParcel:res.data})\n            this.setState({setModalParcel:false})\n        }).catch((err)=>{\n            console.log(err)\n        })  \n    \n    }\n\n    renderCategoryParcel=()=>{\n        return this.state.categoryParcel.map((val,index)=>{\n            return (\n                <>\n                    <TableRow key={val.id}>\n                        <TableCell>{index+1}</TableCell>\n                        <TableCell>{val.id}</TableCell>\n                        <TableCell>{val.nama}</TableCell>\n                        <TableCell>\n                        <button onClick={()=>this.onDelete(val.id)}>Delete</button>\n                        </TableCell>\n\n                    </TableRow>\n                </>\n            )\n        })\n    }\n\n    pagination=()=>{\n